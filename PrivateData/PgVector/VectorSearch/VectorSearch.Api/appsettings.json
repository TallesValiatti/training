{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Application":   {
    "SearchThreshold": 0.20
  },
  "AzureOpenAI": {
    "Url": "https://<YOUR-SERVICE-NAME>.openai.azure.com/",
    "Key": "<YOUR-KEY>"
  },
  "ConnectionStrings": {
    "DefaultConnection": "<YOUR-CONNECTION-STRING>"
  }
}
